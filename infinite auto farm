while true do
for i,v in pairs(game.Workspace.CurrencyZones.Home.Drops.Bag:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
for i,v in pairs(game.Workspace.CurrencyZones.Home.Drops.Safe:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
for i,v in pairs(game.Workspace.CurrencyZones.Home.Drops.Single:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
for i,v in pairs(game.Workspace.CurrencyZones.Mars.Drops.Single:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
for i,v in pairs(game.Workspace.CurrencyZones.Mars.Drops.Safe:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
for i,v in pairs(game.Workspace.CurrencyZones.Mars.Drops.Bag:GetChildren()) do
   if v:IsA("MeshPart") then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
   wait(0.1)
   end
end
wait(0.1)
end
